<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springCloud.dao.UserRoleDao">
    <resultMap type="com.springCloud.domain.UserRole" id="UserRole">
        <id property="id" column="id"></id>
        <result property="userId" column="user_id"></result>
        <result property="role" column="role"></result>
    </resultMap>


    <select id="getUserRoleById" parameterType="String" resultMap="UserRole">
        SELECT id,role,user_id FROM user_role WHERE user_id=#{id}
    </select>
    
</mapper>
